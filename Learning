var url = "https://storage.googleapis.com/upload/storage/v1/b/" 
          + bucketName + "/o?uploadType=media&name=" + encodedFileName;

var options = {
  method: "POST",   // âœ… must be POST for /upload/
  contentType: blob.getContentType(),
  payload: blob.getBytes(),
  headers: {
    Authorization: "Bearer " + token
  }
};

var responseUpload = UrlFetchApp.fetch(url, options);
Logger.log(responseUpload.getContentText());
