function uploadLatestExcelToGCS() {

  var folderId = "YOUR_FOLDER_ID";
  var bucketName = "your-gcs-bucket-name";

  // Get latest Excel file sorted by createdTime
  var response = Drive.Files.list({
    q: "'" + folderId + "' in parents and mimeType='application/vnd.openxmlformats-officedocument.spreadsheetml.sheet'",
    orderBy: "createdTime desc",
    pageSize: 1
  });

  if (!response.files || response.files.length === 0) {
    Logger.log("No Excel file found.");
    return;
  }

  var fileId = response.files[0].id;
  var fileName = response.files[0].name;

  var blob = DriveApp.getFileById(fileId).getBlob();
  var encodedFileName = encodeURIComponent(fileName);

  var url = "https://storage.googleapis.com/upload/storage/v1/b/" 
            + bucketName + "/o/" + encodedFileName + "?uploadType=media";

  var token = ScriptApp.getOAuthToken();

  var options = {
    method: "PUT",  // overwrite
    contentType: blob.getContentType(),
    payload: blob.getBytes(),
    headers: {
      Authorization: "Bearer " + token
    }
  };

  var responseUpload = UrlFetchApp.fetch(url, options);

  Logger.log("Upload status: " + responseUpload.getResponseCode());
}
